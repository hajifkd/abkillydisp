{
  "manifest_version": 3,
  "name": "abkillydisp",
  "description": "An browser extension to show killy picture at the AB auction page",
  "version": "0.0.1",
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.aquabid.com/*"
      ],
      "js": [
        "content_script.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}